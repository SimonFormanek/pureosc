{
    "name": "vendor/pureosc",
    "description": "Description of project pureosc.",
    "authors": [
        {
            "name": "PureHTML",
            "email": "pureosc@purehtml.cz"
        }
    ],
    "require": {
        "ext-gettext": "*",
        "ext-intl": "*",
        "spoje.net/flexipeehp": "dev-master",
        "vitexsoftware/ease-framework": "dev-master",
        "vitexsoftware/flexipeehp-bricks": "dev-master",
        "league/commonmark": "dev-master",
        "dompdf/dompdf": "dev-master",
        "adamstipak/webpay-php": "dev-master",
        "phpmailer/phpmailer": "^6.0",
        "ckeditor/ckeditor": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PureOSC\\": "catalog/includes/classes/",
            "PureOSC\\flexibee\\": "catalog/ext/flexibee/",
            "PureOSC\\Admin\\": "admin/includes/classes/",
            "PureOSC\\Admin\\flexibee\\": "admin/ext/flexibee/"
        },
        "classmap": ["catalog/includes/classes/", "catalog/includes/modules/", "catalog/admin/includes/classes/", "catalog/admin/includes/modules/","catalog/admin/ext/aas/classes"]
    },
        "scripts": {
        "post-install-cmd": "make install",
        "post-update": "make install"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,

    "require-dev": {
        "phpstan/phpstan": "^0.10.7",
        "robmorgan/phinx": "dev-master",
        "phpunit/phpunit": "*"
    }
}
